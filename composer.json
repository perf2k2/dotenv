{
  "name": "perf2k2/dotenv",
  "description": "Lightweight and fast alternative to phpdotenv without dependencies",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.26",
    "vlucas/phpdotenv": "4.2.2"
  },
  "autoload": {
    "psr-4": {
      "Perf2k2\\Dotenv\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Perf2k2\\Dotenv\\Tests\\": "tests/"
    }
  }
}
